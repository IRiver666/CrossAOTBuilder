<Project Sdk="Microsoft.Build.NoTargets/3.7.56">

  <PropertyGroup>
   <TargetFrameworks>net8.0;net9.0;net10.0</TargetFrameworks>
   <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
   <PackageId>CrossAOTBuilder</PackageId>
    <Version>1.0.0</Version>
    <Authors>IRiver666</Authors>
    <Description>MSBuild targets to assist cross-compiling native AOT publish for Windows on Linux.</Description>
    <PackageProjectUrl>https://github.com/IRiver666/CrossAOTBuilder</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="CrossAOTBuilder.targets" Pack="true" PackagePath="build/" />
    <Content Include="Crosscompile.LinuxToWin.targets" Pack="true" PackagePath="build/" />
    <Content Include="README.md" Pack="true" PackagePath="/" />
  </ItemGroup>

</Project>
